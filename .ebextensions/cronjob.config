files:
    "/etc/cron.d/schedule_run":
        mode: "000644"
        owner: root
        group: root
        content: |
            * * * * * root . /opt/elasticbeanstalk/support/envvars; /usr/local/bin/php /crud-urls/artisan schedule:run >> /crud-urls/storage/logs/laravel.log 2>&1

commands:
    remove_old_cron:
        command: "rm -f /etc/cron.d/*.bak"
